<Window x:Class="ePlanifv2.ReportWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ePlanifv2"
		xmlns:rv="clr-namespace:Microsoft.Reporting.WinForms;assembly=Microsoft.ReportViewer.WinForms"
        mc:Ignorable="d" Loaded="Window_Loaded" WindowStartupLocation="CenterOwner"
        Title="ReportWindow" Height="600" Width="800" Icon="Images/calendar.png">
	<WindowsFormsHost>
		<rv:ReportViewer x:Name="reportViewer" ProcessingMode="Remote"/>
	</WindowsFormsHost>
</Window>
